{
    "name": "typescript-express-mongo-ethereum-starter",
    "version": "1.0.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/niksauer/TypescriptExpressMongoEthereumStarter.git"
    },
    "author": "Niklas Sauer",
    "scripts": {
        "start": "npm run serve",
        "build": "npm run build-ts && npm run lint",
        "serve": "node dist/server.js",
        "watch-node": "nodemon --ignore 'config/*' dist/server.js",
        "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
        "test": "jest --config ./jest.config.js --runInBand --detectOpenHandles --forceExit",
        "watch-test": "npm run test -- --watchAll",
        "build-ts": "tsc",
        "watch-ts": "tsc -w",
        "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
        "debug": "npm run build && npm run watch-debug",
        "serve-debug": "nodemon --ignore 'config/*' --inspect dist/server.js",
        "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
    },
    "dependencies": {
        "axios": "^0.21.1",
        "big.js": "^5.2.2",
        "body-parser": "^1.19.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "ethers": "^4.0.39",
        "express": "^4.17.1",
        "express-async-errors": "^3.1.1",
        "express-paginate": "^1.0.0",
        "express-validator": "^6.2.0",
        "file-type": "^12.4.0",
        "jsend": "^1.1.0",
        "jwks-rsa": "^1.6.0",
        "jwt-decode": "^2.2.0",
        "loglevel": "^1.6.6",
        "moment": "^2.24.0",
        "mongoose": "^5.7.10",
        "multer": "^1.4.2",
        "passport": "^0.4.0",
        "passport-anonymous": "^1.0.1",
        "passport-jwt": "^4.0.0",
        "read-chunk": "^3.2.0",
        "reflect-metadata": "^0.1.13",
        "swagger-ui-express": "^4.1.2",
        "tsyringe": "^4.0.1",
        "uuid": "^3.3.3",
        "winston": "^3.2.1",
        "yamljs": "^0.3.0"
    },
    "devDependencies": {
        "@types/big.js": "^4.0.5",
        "@types/body-parser": "^1.17.1",
        "@types/chai": "^4.2.4",
        "@types/compression": "^1.0.1",
        "@types/concurrently": "^4.1.0",
        "@types/cors": "^2.8.6",
        "@types/dotenv": "^6.1.1",
        "@types/errorhandler": "^0.0.32",
        "@types/eslint": "^6.1.3",
        "@types/express": "^4.17.2",
        "@types/express-paginate": "^1.0.0",
        "@types/jest": "^24.0.22",
        "@types/jsend": "^1.0.28",
        "@types/jwt-decode": "^2.2.1",
        "@types/mongoose": "^5.5.30",
        "@types/multer": "^1.3.10",
        "@types/node": "^12.12.7",
        "@types/nodemon": "^1.19.0",
        "@types/passport": "^1.0.1",
        "@types/passport-anonymous": "^1.0.3",
        "@types/passport-jwt": "^3.0.3",
        "@types/supertest": "^2.0.8",
        "@types/swagger-ui-express": "^4.1.0",
        "@types/uuid": "^3.4.6",
        "@types/yamljs": "^0.2.30",
        "@typescript-eslint/eslint-plugin": "^2.7.0",
        "@typescript-eslint/parser": "^2.7.0",
        "chai": "^4.2.0",
        "concurrently": "^4.1.2",
        "eslint": "^6.6.0",
        "jest": "^24.9.0",
        "nodemon": "^1.19.4",
        "supertest": "^4.0.2",
        "ts-jest": "^24.1.0",
        "ts-node": "^8.5.0",
        "typescript": "^3.7.2"
    },
    "optionalDependencies": {
        "@shelf/jest-mongodb": "^1.1.3",
        "ganache-core": "^2.8.0",
        "mongodb": "^3.3.5"
    }
}
